{
  "name": "fhir",
  "version": "2.0.10",
  "description": "Node.JS library for serializing/deserializing FHIR resources between JS/JSON and XML using various node.js XML libraries. Validating XML against the schemas is supported. Basic JS validation is supported against the core FHIR profile definitions.",
  "readme": "README.md",
  "bugs": {
    "url": "https://github.com/lantanagroup/FHIR.js/issues",
    "email": "sean.mcilvenna@lantanagroup.com"
  },
  "license": "ISC",
  "author": "Sean McIlvenna (sean.mcilvenna@lantanagroup.com)",
  "contributors": [
    {
      "name": "Sean McIlvenna",
      "email": "sean.mcilvenna@lantanagroup.com"
    }
  ],
  "keywords": [
    "fhir",
    "healthcare",
    "interoperability",
    "xml",
    "json",
    "serialization",
    "hl7",
    "standards",
    "clinical"
  ],
  "main": "fhir.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lantanagroup/FHIR.js/"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "libxmljs": "^0.14.3",
    "lodash": "^3.10.1",
    "path": "^0.12.7",
    "q": "^1.4.1",
    "xml2js": "^0.4.9",
    "xmlbuilder": "^2.6.4"
  },
  "devDependencies": {
    "mocha": "^2.3.2",
    "xmldom": "^0.1.19",
    "xpath": "0.0.9",
    "yargs": "^3.25.0"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha test/*.js",
    "prepublish": "node profiles/packageProfiles.js -d profiles/dstu2 -o profiles/dstu2.js"
  },
  "bundleDependencies": [
    "libxmljs",
    "lodash",
    "path",
    "q",
    "xml2js",
    "xmlbuilder"
  ]
}
